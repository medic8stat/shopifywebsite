        "bp-screening": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Blood Pressure Screening",
            "content": "<p>Comprehensive blood pressure evaluation including resting, orthostatic, and ambulatory monitoring options. Identifies hypertension patterns that standard office readings may miss.</p>",
