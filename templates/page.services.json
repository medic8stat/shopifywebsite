    "assessments-cardiopulmonary": {
      "type": "section-accordion-group",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "title": "Cardiopulmonary & Cardiovascular"
          }
        },
        "vo2-max": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "VO2 Max Testing",
            "content": "<p>Gold-standard assessment of aerobic fitness and cardiovascular capacity. This test measures your body's maximum oxygen uptake during intense exercise, providing critical insights into your cardiovascular health and endurance potential.</p>",
            "show_icon": true,
            "icon_name": "icon-heart",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "submaximal": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Submaximal Fitness Testing",
            "content": "<p>A less intensive alternative to VO2 max testing that estimates cardiovascular fitness without pushing to maximum exertion. Ideal for those new to fitness testing or with certain health considerations.</p>",
            "show_icon": true,
            "icon_name": "icon-heart",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "ecg": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Resting ECG",
            "content": "<p>Electrocardiogram recording of your heart's electrical activity at rest. Screens for arrhythmias, conduction abnormalities, and other cardiac conditions that may require attention.</p>",
            "show_icon": true,
            "icon_name": "icon-heart",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "exercise-ecg": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Exercise ECG (Stress Test)",
            "content": "<p>Monitors heart rhythm and function during physical exertion. Reveals how your heart responds to stress and can identify issues not apparent at rest, including exercise-induced arrhythmias.</p>",
            "show_icon": true,
            "icon_name": "icon-heart",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        }
      },
      "block_order": [
        "heading",
        "vo2-max",
        "submaximal",
        "ecg",
        "exercise-ecg"
      ],
      "settings": {
        "heading_font_size": "heading-small",
        "question_font_size": "heading-x-small",
        "width": "wrapper",
        "columns": "1",
        "color_scheme": "scheme-1",
