      "type": "section-accordion-group",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "title": "Neurological, Cognitive & Sensory"
          }
        },
        "vr-vision": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "VR Vision Assessment",
            "content": "<p>Advanced virtual reality-based vision testing that goes beyond standard eye exams. Evaluates visual processing, peripheral vision, depth perception, and visual-motor integration.</p>",
            "show_icon": true,
            "icon_name": "icon-flare",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "hearing": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Hearing Assessment",
            "content": "<p>Comprehensive audiometric testing to evaluate hearing across all frequencies. Early detection of hearing changes allows for intervention before significant impact on quality of life.</p>",
            "show_icon": true,
            "icon_name": "icon-chat",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "reaction-time": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Reaction Time Testing",
            "content": "<p>Measures cognitive processing speed and neuromuscular response time. Important indicator of brain health and nervous system function, with implications for safety and performance.</p>",
            "show_icon": true,
            "icon_name": "icon-lightning",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        },
        "balance": {
          "type": "question",
          "settings": {
            "default_open": false,
            "title": "Balance Assessment",
            "content": "<p>Evaluates vestibular function, proprioception, and postural stability. Critical for fall prevention and maintaining independence, especially as we age.</p>",
            "show_icon": true,
            "icon_name": "icon-check-circle",
            "icon_size": 20,
            "icon_color": "#1e3a5f"
          }
        }
      },
